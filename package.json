{
  "name": "datastore-car",
  "version": "1.2.0",
  "description": "Content ARchive format reader and writer",
  "main": "car.js",
  "type": "module",
  "browser": "./dist/cjs-browser/car-browser.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "limbo test/test-*.js && cp test/go.car dist/cjs-browser/test/ && cp test/go.car dist/cjs-node/test/",
    "prepublishOnly": "npm run build",
    "lint": "standard",
    "test:cjs": "rm -rf dist && npm run build && mocha dist/cjs-node/test/test-*.cjs && npm run test:cjs:browser",
    "test:node": "hundreds mocha test/test-*.js",
    "test:cjs:browser": "polendina --cleanup dist/cjs-browser/test/test-*.js",
    "test": "npm run lint && npm run test:node && npm run test:cjs",
    "coverage": "c8 --reporter=html --reporter=text mocha test/test-*.js && npx st -d coverage -p 8888",
    "docs": "jsdoc4readme --readme *.js lib/raw.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvagg/js-datastore-car.git"
  },
  "keywords": [
    "car",
    "ipld",
    "ipfs"
  ],
  "author": "Rod <rod@vagg.org> (http://r.va.gg/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rvagg/js-datastore-car/issues"
  },
  "homepage": "https://github.com/rvagg/js-datastore-car#readme",
  "devDependencies": {
    "@ipld/block": "^6.0.1",
    "@ipld/dag-cbor": "^1.1.4",
    "@mikeal/limbo": "0.0.1",
    "assert": "^2.0.0",
    "bl": "^4.0.2",
    "cids": "^1.0.0",
    "garbage": "0.0.0",
    "hundreds": "0.0.7",
    "ipld-dag-pb": "^0.20.0",
    "jsdoc4readme": "^1.3.0",
    "mocha": "^8.1.1",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "interface-datastore": "^2.0.0",
    "multiformats": "^2.3.0",
    "varint": "^5.0.0"
  },
  "exports": {
    ".": {
      "import": "./car.js",
      "require": "./dist/cjs-node/car.cjs"
    },
    "./car.js": {
      "import": "./car.js",
      "require": "./dist/cjs-node/car.cjs"
    },
    "./car-browser.js": {
      "import": "./car-browser.js",
      "require": "./dist/cjs-node/car-browser.cjs"
    }
  }
}
